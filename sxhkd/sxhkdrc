XF86AudioLowerVolume
	pamixer --decrease 5 

XF86AudioRaiseVolume
	pamixer --increase 5

XF86AudioMute
	pamixer -t

XF86MonBrightnessUp
	brightnessctl -d intel_backlight s +100

XF86MonBrightnessDown
	brightnessctl -d intel_backlight s 100-

XF86Audio{Prev,Next}
  playerctl {previous,next}

XF86AudioPlay
  playerctl play-pause

XF86AudioStop
  playerctl play-pause

super + shift + l
  betterlockscreen --lock
